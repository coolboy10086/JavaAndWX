page{
  background-color: #eee;
}
.aa,.bb{
  width: 180rpx;
  padding: 4rpx 20rpx;
  height: 50rpx;
  background-color: #f00;
  color: #fff;
  margin-top: 10rpx;
}
.bb{
  border-radius: 0rpx 0rpx 0rpx 20rpx;
}
#sw{
  width: 100%;
  height: 150px;
}
swiper-item{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 45pt;
  color: #fff;
}
swiper-item:nth-child(1){
  background-color: #f50;
}
swiper-item:nth-child(2){
  background-color: #5f0;
}
swiper-item:nth-child(3){
  background-color: #05f;
}
#tips{
  width: 400rpx;
  height: 80rpx;
  margin: 30rpx auto; 
  padding: 20rpx;
  border-radius: 20rpx;
  color: #012;
}
#tips text{
  font-size: 48rpx;
  padding:0rpx 20rpx;
}
.ttt{
  background-color: #9f9;
}
.kkk{
  background-color: #f99;
}
